[color]
	ui = true
[alias]
        ci = commit
        co = checkout
        s = status -sb
        d = diff
        r = remote
        b = branch
        l = log --pretty=format:'%Cred%h%Creset%x09  %Cgreen%ad%Creset%x09%C(bold blue)%an%Creset: %s'
        graph = !git l --graph
        mylog = !git l --author=\"$(git config user.name)\"
        shove = push origin HEAD
        ready = rebase -i @{u}
        staged = diff --staged
[core]
        excludesfile = ~/.gitignore_global
[http]
        sslVerify = false
[mergetool]
	keepBackup = true
[log]
        date = short
        abbrevCommit = true
[help]
	autocorrect = 1
[include]
        path = ~/.gituser
