[color]
        ui = true
[alias]
        ci = commit
        co = checkout
        pl = pull
        ph = push
        s = status -sbu
        d = diff
        wd = diff --word-diff
        r = remote
        b = branch
        l = log --pretty=format:'%Cred%h%Creset%x09  %Cgreen%ad%Creset%x09%C(bold blue)%an%Creset: %s'
        graph = !git l --graph
        mylog = !git l --author=\"$(git config user.name)\"
        shove = push origin HEAD
        ready = rebase -i @{u}
        staged = diff --staged
        amend = commit --amend -C HEAD
        undo = reset --soft HEAD^
        skip = update-index --skip-worktree
        unskip = update-index --no-skip-worktree
        skipped = !git ls-files -v | grep "^S " | sed 's/^S //g'
        assume = update-index --assume-unchanged
        unassume = update-index --no-assume-unchanged
        assumed = !git ls-files -v | grep "^h " | sed 's/^h //g'
        changed = diff --name-only --diff-filter=M
[core]
        autocrlf = true
        excludesfile = ~/.gitignore
[branch]
        autosetuprebase = always
[http]
        sslVerify = false
[mergetool]
        keepBackup = true
[push]
        default = simple
[diff]
        algorithm = patience
[log]
        date = short
        abbrevCommit = true
[rerere]
        enabled = 1
[help]
        autocorrect = 1
[include]
        path = ~/.gitlocal
